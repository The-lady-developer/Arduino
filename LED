int switchPin = 8;
int ledPin = 13;
boolean lastButton = LOW;
boolen ledOn = FALSE;
void setup() {
  pinMode (switchPin, INPUT);
  pinMode(ledPin, OUTPUT);
}

void loop() {
  if (digitalRead(switchPin) == HIGH && lastButton == LOW)
  {  
  ledOn = !ledOn;
  lastButton = HIGH;
    }
    else
    {
     lastButton = digitalRead(switchPin);
    }
    digitalWrite(ledPin, ledOn);
}
